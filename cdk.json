{
    "app": "python app.py",
    "watch": {
        "include": [
            "**"
        ],
        "exclude": [
            "README.md",
            "cdk*.json",
            "requirements*.txt",
            "source.bat",
            "**/__pycache__",
            "**/*.pyc"
        ]
    },
    "context": {
        "@aws-cdk/core:enableStackNameDuplicates": "true",
        "aws-cdk:enableDiffNoFail": "true"
    }
}